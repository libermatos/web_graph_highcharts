<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="view_area_basic_hc_graph">
			<field name="name">demo.area.basic.graph</field>
			<field name="model">demo.graph.hc</field>
			<field name="type">graph_highcharts</field>
			<field name="arch" type="xml">
				<graph_highcharts string="Area Basic" method="area_basic"/>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_area_basic_hc">
			<field name="res_model">demo.graph.hc</field>
			<field name="view_type">form</field>
			<field name="view_mode">graph_highcharts</field>
			<field name="search_view_id" ref="demo_graph_highcharts.demo_graph_hc_search"/>
			<field name="view_id" ref="demo_graph_highcharts.view_area_basic_hc_graph"/>
		</record>

		<menuitem action="action_area_basic_hc" name="Area Basic" id="menu_area_basic_hc" parent="menu_hc_area charts_root" sequence="1"/>
	</data>
</openerp>
